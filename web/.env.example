# Configurações de ambiente para o Frontend
# Copie este arquivo para .env.local (desenvolvimento) ou .env.production (produção)

# API URL - Ajuste para o endereço correto da sua API
NEXT_PUBLIC_API_URL=http://localhost:3001

# Autenticação
NEXT_PUBLIC_AUTH_ENABLED=true

# Análise e Monitoramento
NEXT_PUBLIC_ANALYTICS_ENABLED=false

# Feature Flags
NEXT_PUBLIC_FEATURE_RELATORIOS_AVANCADOS=false
NEXT_PUBLIC_FEATURE_DASHBOARD_COMPLETO=false 